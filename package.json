{
  "name": "Hallets-Author-Website",
  "version": "0.1.0",
  "description": "Author Website",
  "main": "server.js",
  "homepage": "https://proj3-author-website.herokuapp.com/",
  "dependencies": {
    "@elsdoerfer/react-arrow": "^1.0.4",
    "babel-jest": "^24.9.0",
    "bcryptjs": "^2.4.3",
    "bootstrap-react": "^0.5.0-alpha.14",
    "concurrently": "^4.1.0",
    "connect-session-sequelize": "^7.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "jest": "^24.9.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-auth0": "^1.4.0",
    "passport-local": "^1.0.0",
    "react": "^17.0.1",
    "sequelize": "^6.5.0",
    "webpack": "^4.42.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server --unhandled-rejections=strict",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "14.15.0",
    "npm": "6.14.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kcushing1/Halletts-Author-Website.git"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
